{
    "rules": {
        "indent": 0,
        "no-await-in-loop": 0,
        "require-atomic-updates": 0,
        "no-dupe-else-if": 0,
        "no-import-assign": 0,
        "no-setter-return": 0
    },
    "globals": {
        "BigInt": true
    },
    "extends": ["nodemailer", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018
    }
}
